{
  "name": "remindeer",
  "version": "1.0.0",
  "description": "A node api for the remindeer project",
  "main": "./app/app.ts",
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon --exec 'npx tsc && node ./bin/www'",
    "start": "npx tsc && node ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngareleo/remindeer-node-api.git"
  },
  "keywords": [
    "typescript",
    "node",
    "mongodb"
  ],
  "author": "ngare",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngareleo/remindeer-node-api/issues"
  },
  "homepage": "https://github.com/ngareleo/remindeer-node-api#readme",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/express-slow-down": "^1.3.2",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-slow-down": "^1.6.0",
    "http-errors": "^2.0.0",
    "mongoose": "^7.3.4",
    "morgan": "^1.10.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "nodemonConfig": {
    "ext": "ts js json",
    "watch": [
      "app",
      "node_modules"
    ]
  }
